<div class="container">
  <div class="row"></div>
  <div class="row"></div>
  <div class="row">
    <div class="col m6 offset-m3 center-align">
      <h3>{{monsterTruck.truckDeets.name}}</h3>
      <img src="{{ monsterTruck.truckDeets.truck_pic }}"alt="{{ monsterTruck.truckDeets.name }}">
      <h5>Food: {{ monsterTruck.truckDeets.food }}</h5>
      <h6 class="red-text">Safety Rating: {{ monsterTruck.truckDeets.safety }}</h6>
      {{#each monsterTruck.schedDeets }}
      <h6>bar name{{ this.name }}</h6>
      {{/each}}
    </div>
  </div>
</div>

<div class="container">
  <div class="row">
    <div class="col s8 m8 offset-m2">
      <section>
        <table class="highlight responsive-table">
          <thead>
            <tr>
              <th>User</th>
              <th>Review</th>
            </tr>
          </thead>
          <tbody>
            {{#each monsterTruck.commentDeets.review }}
            <tr>
              <td>username</td>
              <td>{{ this.this.review }}</td>
            </tr>
            {{/each}}
          </tbody>
        </table>
      </section>
    </div>
  </div>
</div>

<div class="row">
  <section>
    {{> _form}}
  </section>
</div>
